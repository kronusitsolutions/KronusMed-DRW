{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "next build",
    "typecheck": "tsc --noEmit",
    "lint:ci": "next lint --max-warnings=0",
    "ci:check": "pnpm run db:generate && pnpm run typecheck && pnpm run lint:ci && pnpm run build",
    "dev": "next dev -H 0.0.0.0 -p 3000",
    "lint": "next lint",
    "start": "next start",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate-deploy": "prisma migrate deploy",
    "db:seed": "tsx scripts/seed.ts",
    "db:create-initial-admin": "tsx scripts/create-initial-admin.ts",
    "db:setup-production-admin": "node scripts/setup-production-admin.js",
    "railway:seed": "node scripts/railway-seed.js",
    "railway:setup": "node scripts/railway-setup.js",
    "db:studio": "prisma studio",
    "docker:dev": "chmod +x scripts/docker-dev.sh && ./scripts/docker-dev.sh",
    "docker:start": "docker-compose up -d postgres",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f postgres",
    "docker:backup": "./scripts/docker-dev.sh backup",
    "docker:restore": "./scripts/docker-dev.sh restore",
    "docker:clean": "./scripts/docker-dev.sh clean",
    "docker:dev:start": "docker-compose -f docker-compose.dev.yml --profile dev up -d",
    "docker:dev:stop": "docker-compose -f docker-compose.dev.yml down",
    "docker:dev:logs": "docker-compose -f docker-compose.dev.yml logs -f app",
    "docker:dev:rebuild": "docker-compose -f docker-compose.dev.yml build --no-cache app",
    "docker:dev:full": "chmod +x scripts/docker-dev-full.sh && ./scripts/docker-dev-full.sh",
    "security:audit": "node scripts/security-audit.js",
    "security:update": "node scripts/update-dependencies.js",
    "security:check": "npm audit && node scripts/security-audit.js",
    "db:verify-users": "node scripts/verify-users.js",
    "backup:secure": "chmod +x scripts/secure-backup.sh && ./scripts/secure-backup.sh create",
    "backup:list": "chmod +x scripts/secure-backup.sh && ./scripts/secure-backup.sh list",
    "backup:verify": "chmod +x scripts/secure-backup.sh && ./scripts/secure-backup.sh verify",
    "backup:cleanup": "chmod +x scripts/secure-backup.sh && ./scripts/secure-backup.sh cleanup",
    "secrets:generate": "powershell -ExecutionPolicy Bypass -File scripts/generate-secrets-simple.ps1",
    "secrets:check": "powershell -ExecutionPolicy Bypass -File scripts/generate-secrets.ps1 check",
    "migrate:node18": "powershell -ExecutionPolicy Bypass -File scripts/migrate-to-node18.ps1",
    "verify:node18": "powershell -ExecutionPolicy Bypass -File scripts/verify-node18.ps1",
    "update:node22": "powershell -ExecutionPolicy Bypass -File scripts/update-to-node22.ps1",
    "verify:node22": "powershell -ExecutionPolicy Bypass -File scripts/verify-node22-compatibility.ps1",
    "test:node22": "powershell -ExecutionPolicy Bypass -File scripts/test-node22-setup.ps1",
    "check:compatibility": "powershell -ExecutionPolicy Bypass -File scripts/check-dependencies-compatibility.ps1",
    "verify:railway": "powershell -ExecutionPolicy Bypass -File scripts/verify-railway-deployment.ps1",
    "optimize:performance": "node scripts/optimize-performance.js",
    "db:optimize": "npx prisma migrate deploy && node scripts/optimize-performance.js",
    "test:database": "node scripts/test-database-connection.js",
    "fix:encrypted-data": "node scripts/fix-encrypted-data.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@hookform/resolvers": "^3.10.0",
    "@prisma/client": "^6.15.0",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "1.1.1",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-context-menu": "2.2.4",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-hover-card": "1.1.4",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-menubar": "1.1.4",
    "@radix-ui/react-navigation-menu": "1.2.3",
    "@radix-ui/react-popover": "1.1.4",
    "@radix-ui/react-progress": "1.1.1",
    "@radix-ui/react-radio-group": "1.2.2",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slider": "1.2.2",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/react-tabs": "1.1.2",
    "@radix-ui/react-toast": "1.2.4",
    "@radix-ui/react-toggle": "1.1.1",
    "@radix-ui/react-toggle-group": "1.1.1",
    "@radix-ui/react-tooltip": "1.1.6",
    "@types/bcryptjs": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "date-fns": "4.1.0",
    "embla-carousel-react": "8.5.1",
    "exceljs": "^4.4.0",
    "geist": "^1.3.1",
    "input-otp": "1.4.1",
    "lucide-react": "^0.454.0",
    "next": "15.2.5",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "prisma": "^6.15.0",
    "react": "^19",
    "react-day-picker": "9.8.0",
    "react-dom": "^19",
    "react-hook-form": "^7.60.0",
    "react-is": "^19.1.1",
    "react-resizable-panels": "^2.1.7",
    "recharts": "latest",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.0",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.9",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "9.36.0",
    "eslint-config-next": "15.5.3",
    "postcss": "^8.5",
    "tailwindcss": "^4.1.9",
    "tsx": "^4.20.3",
    "tw-animate-css": "1.3.3",
    "typescript": "^5.7"
  }
}
